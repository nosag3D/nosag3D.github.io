   this.manejar = function(objCas){
      this.objCas = objCas;

      if (objCas) {
         var id = getNumber(objCas);
         var item = getItem(objCas)*1;
         var valores = new Array(objCas.value);
         var valoresNoPermitidosNatural = new Array("9499","9412");
